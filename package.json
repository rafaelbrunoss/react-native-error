{
  "name": "navigator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "fix:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "start": "react-native start",
    "android": "ENVFILE=.env.dev react-native run-android --active-arch-only",
    "android:windows": "react-native run-android --active-arch-only",
    "build:stg:android": "cd android && ENVFILE=.env.stg ./gradlew bundleRelease -x bundleReleaseJsAndAssets",
    "build:prod:android": "cd android && ENVFILE=.env.prod ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "code-push:keys": "appcenter codepush deployment list -a {user}/{app name} --displayKeys",
    "code-push:android:dev": "appcenter codepush release-react -a {user}/{app name} -d Development",
    "code-push:android:stg": "appcenter codepush release-react -a {user}/{app name} -d Staging",
    "code-push:android:prod": "appcenter codepush promote -a {user}/{app name} -d Production",
    "ios": "react-native run-ios",
    "gradle:update": "cd android && ./gradlew wrapper --gradle-version 7.5.1 --distribution-type all",
    "gradle:clean": "cd android && ./gradlew clean",
    "debugger": "cd ~/flipper && ./flipper",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "test:ci": "jest --ci --runInBand",
    "e2e:build:android": "detox build",
    "e2e:run:android": "detox test",
    "snyk": "snyk",
    "snyk:test": "snyk test",
    "snyk:fix": "snyk fix",
    "sonarqube": "docker compose up",
    "sonar-scanner": "sonar-scanner",
    "format": "prettier --write . --ignore-path .prettierignore",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js,.jsx src/ --fix",
    "lint-folder": "eslint -c .eslintrc.js --fix --ext .ts,.tsx,.js,.jsx",
    "stylelint": "stylelint \"**/*.scss\" --fix",
    "commitlint": "npx --no -- commitlint --edit $1",
    "commitlint:windows": "npx commitlint --edit $1",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@react-navigation/drawer": "6.5.0",
    "@react-navigation/native": "6.0.13",
    "@react-navigation/native-stack": "6.9.1",
    "@reduxjs/toolkit": "1.9.0",
    "aws-sdk": "2.1257.0",
    "axios": "0.27.2",
    "axios-retry": "3.3.1",
    "base-64": "1.0.0",
    "buffer": "6.0.3",
    "formik": "2.2.9",
    "moment": "2.29.4",
    "platform": "1.3.6",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-blob-util": "0.17.0",
    "react-native-config": "1.4.11",
    "react-native-encrypted-storage": "4.0.3",
    "react-native-fast-image": "8.6.3",
    "react-native-get-random-values": "1.8.0",
    "react-native-image-viewing": "0.2.2",
    "react-native-pager-view": "6.1.0",
    "react-native-pdf": "6.6.2",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "3.18.2",
    "react-native-tab-view": "3.3.0",
    "react-native-uuid": "2.0.1",
    "react-native-vector-icons": "9.2.0",
    "react-native-video": "6.0.0-alpha.3",
    "react-native-webview": "11.23.1",
    "react-redux": "8.0.5",
    "realm": "11.2.0",
    "redux": "4.2.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@babel/runtime": "7.19.0",
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@expo/metro-config": "0.5.1",
    "@react-native-community/eslint-config": "3.1.0",
    "@testing-library/jest-native": "5.0.0",
    "@testing-library/react-native": "11.2.0",
    "@tsconfig/react-native": "2.0.2",
    "@types/base-64": "1.0.0",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/platform": "1.3.4",
